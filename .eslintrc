{
  "extends": "airbnb/base",

  "rules": {
    "space-before-function-paren": [2, "always"],
    "no-loop-func": [0],
    "no-else-return": [0],
    "no-param-reassign": [0],
    "no-use-before-define": [0],
    "comma-dangle": [2, "never"],
    "guard-for-in": [0],
    "no-shadow": [0],
    "no-console": [0]
  }
}
