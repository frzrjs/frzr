{
  "name": "frzr",
  "version": "0.8.8",
  "description": "FRZR view library",
  "jsnext:main": "src/index.js",
  "browser:": "dist/frzr.js",
  "main": "dist/frzr-server.js",
  "scripts": {
    "start": "node watch",
    "build": "npm run test && npm run build-server-js",
    "build-js": "rollup -n frzr -f umd -i src/index.js -o dist/frzr.js && uglifyjs dist/frzr.js -cmo dist/frzr.min.js",
    "build-server-js": "rollup -n frzr -f cjs -i src/server/index.js -o dist/frzr-server.js",
    "test": "npm run build-js && npm run build-server-js && tape test/server.js && browserify test/index.js | tape-run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pakastin/frzr.git"
  },
  "keywords": [
    "frzr",
    "view",
    "library"
  ],
  "author": "Juha Lindstedt",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pakastin/frzr/issues"
  },
  "homepage": "https://github.com/pakastin/frzr#readme",
  "devDependencies": {
    "browserify": "^13.0.0",
    "chokidar": "^1.4.1",
    "rollup": "^0.24.0",
    "tape": "^4.2.2",
    "tape-run": "^2.1.0",
    "uglify-js": "^2.6.1"
  }
}
